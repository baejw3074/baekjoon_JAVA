import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int count = 0;
        while(n>=5){
            int num = n/5;
            count += num;
            n = n/5;
        }
        System.out.println(count);
    }
}
