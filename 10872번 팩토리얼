import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int total = 1;
		while(true) {
			if(num == 0) {
				break;
			}
			total = num * total;
			num = num - 1;
		}
		System.out.print(total);
		sc.close();
		
	}
}
