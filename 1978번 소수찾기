import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		int num_count = 0;
		for(int i=0; i<t; i++) {
			int num = sc.nextInt();
			int count = 0;
			for(int j=1; j<=num; j++) {
				if(num %j == 0) {
					count +=1;
					
				}
			}
			if(count == 2) {
				num_count +=1;
			}
		}
		System.out.print(num_count);
		sc.close();
		
	}
}
