import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str = sc.next();
		int[] arr = new int[26];
		for(int i=0; i<26; i++) {
			arr[i] = -1;
		}
		for(int i=0; i<str.length(); i++) {
			int num = (int)str.charAt(i) - 97;
			if(arr[num] != -1) {
				continue;
			}
			arr[num] = i;
			
		}
		for(int i=0; i<26; i++) {
			System.out.print(arr[i] + " ");
		}
		sc.close();
	}
}
