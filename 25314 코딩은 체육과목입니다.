import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int count = 0;
        while(num!=0){
            num = num - 4;
            count +=1;
        }
        String str = "";
        for(int i=0; i<count; i++){
            str += "long ";
        }
        System.out.println(str + "int");
    }
}
